{
  "name": "tochka-developers/jsonrpc",
  "description": "JsonRpc extension for Laravel",
  "license": "MIT",
  "homepage" : "https://github.com/xdloc/jsonrpc",
  "authors": [
    {
      "name": "Pavel Babushkin",
      "email": "darkdarin@tochka.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "laravel/framework": "~5.6",
    "tochka-developers/jsonrpc-smd": "^1.0",
    "phpdocumentor/reflection-docblock": "~4.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Tochka\\JsonRpc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tochka\\JsonRpc\\JsonRpcServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-update-cmd": "Tochka\\JsonRpc\\Installer::run",
    "post-install-cmd": "Tochka\\JsonRpc\\Installer::run"
  }
}
